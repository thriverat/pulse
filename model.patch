+                # Check insights
+                insights = analytics["insights"]
+                if isinstance(insights, list):
+                    print(f"‚úÖ Analytics: Insights validation successful ({len(insights)} insights)")
+                else:
+                    print("‚ùå Analytics: Insights not in correct format")
+                
+                # Check sleep-focus correlation
+                sleep_insights = [i for i in insights if i.get("type") == "sleep_focus"]
+                if sleep_insights:
